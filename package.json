{
  "name": "anon-kafka-mirror",
  "version": "1.3.2",
  "main": "index.js",
  "repository": "git@github.com:nodefluent/anon-kafka-mirror.git",
  "author": "nodefluent",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "clean": "rimraf dist",
    "docker:build": "docker-compose build --no-cache",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "docker": "yarn docker:build && yarn docker:up",
    "lint": "tslint \"lib/**/*.ts\" --project tsconfig.json",
    "setup": "docker-compose up -d",
    "start": "node dist/index.js",
    "test:all": "yarn build && yarn lint && yarn test:cov",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch",
    "test": "mocha",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/murmurhash": "^0.0.0",
    "@types/node": "^10.10.1",
    "@types/pino": "^5.6.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "commander": "^2.18.0",
    "debug": "^4.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "gc-profiler": "^1.4.0",
    "immutable": "^3.8.2",
    "kafka-streams": "^4.6.3",
    "murmurhash": "^0.0.2",
    "pino": "^5.6.1",
    "pino-pretty": "^2.2.0",
    "prom-client": "^11.1.2"
  },
  "preferGlobal": true,
  "bin": {
    "anon-kafka-mirror": "dist/index.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**",
      "dist/**",
      "lib/config/default.ts",
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true
  }
}
